{
  "name": "@paypay-chellenge/server",
  "version": "1.0.0",
  "scripts": {
    "watch-ts": "tsc -w",
    "watch-js": "nodemon ./dist/index.js",
    "build": "tsc",
    "dev": "concurrently \"npm:watch-ts\" \"npm:watch-js\"",
    "start": "node ./dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/node": "^13.9.3",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2",
    "typescript": "^3.8.3"
  }
}
